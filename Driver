
public class Driver {

	public static void main(String[] args) {
		int[] a =  {1,9,5,3,2,8,7,6,4};
		
		for(int i: a)
			System.out.print(i);
		System.out.println();
		
		RandomizedSelect select = new RandomizedSelect();
		
		//smallest order statistic
		System.out.println(select.randomizedSelect(a, 0, a.length-1, 1));
		
		//median order statistic
		System.out.println(select.randomizedSelect(a, 0, a.length-1, a.length/2));
		
		//largest order statistic
		System.out.println(select.randomizedSelect(a, 0, a.length-1, a.length));
		
		IterativeRandomSelect selectIter = new IterativeRandomSelect();
		//smallest order statistic
		System.out.println(selectIter.randomizedSelectSmall(a, 0, a.length-1, 1));
		

	}

}
